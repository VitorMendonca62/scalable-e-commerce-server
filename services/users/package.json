{
  "name": "library-with-nestjs",
  "version": "0.30.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test vitest --config vitest.config.ts",
    "test:cov": "cross-env NODE_ENV=test vitest --coverage --config vitest.config.ts",
    "test:unit": "cross-env NODE_ENV=test vitest --config vitest.config.ts --testPathPattern=\".unit.spec.ts\"",
    "test:unit:cov": "cross-env NODE_ENV=test vitest --coverage --config vitest.config.ts --testPathPattern=\".unit.spec.ts\"",
    "jwt:keys:sign-up": "openssl genrsa -out ./certs/sign-up-private.pem 2048 && openssl rsa -in ./certs/sign-up-private.pem -pubout -out ./certs/sign-up-public.pem",
    "jwt:keys:generate": "pnpm run jwt:keys:sign-up"
  },
  "dependencies": {
    "@fastify/cookie": "^10.0.0",
    "@fastify/static": "^9.0.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^11.1.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.12",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.5",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "amqp-connection-manager": "^4.1.15",
    "amqplib": "^0.10.9",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "fastify": "^5.7.1",
    "handlebars": "^4.7.8",
    "ioredis": "^5.9.2",
    "jose": "^6.1.3",
    "nodemailer": "^7.0.12",
    "openssl-nodejs": "^1.0.5",
    "otp-generator": "^4.0.1",
    "passport": "^0.7.0",
    "pg": "^8.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.28",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.1.12",
    "@types/node": "^20.19.27",
    "@types/nodemailer": "^7.0.4",
    "@types/otp-generator": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-istanbul": "4.0.17",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "vitest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-vitest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "esbuild"
    ]
  }
}
