{
  "name": "auth",
  "version": "0.34.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:prod": "node dist/main",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test vitest --config vitest.config.ts",
    "test:cov": "cross-env NODE_ENV=test vitest --coverage --config vitest.config.ts",
    "test:unit": "cross-env NODE_ENV=test vitest --config vitest.config.ts --testPathPattern=\".unit.spec.ts\"",
    "test:unit:cov": "cross-env NODE_ENV=test vitest --coverage --config vitest.config.ts --testPathPattern=\".unit.spec.ts\"",
    "jwt:keys:generate": "openssl genrsa -out certs/auth-private.pem 2048 && openssl rsa -in certs/auth-private.pem -pubout -out certs/auth-public.pem && openssl genrsa -out certs/reset-pass-private.pem 2048 && openssl rsa -in certs/reset-pass-private.pem -pubout -out certs/reset-pass-public.pem",
    "jwt:keys:clean": "rm certs/*.pem"
  },
  "dependencies": {
    "@fastify/cookie": "^10.0.0",
    "@fastify/static": "^9.0.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^10.4.20",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.12",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/terminus": "^11.0.0",
    "amqp-connection-manager": "^4.1.15",
    "amqplib": "^0.10.9",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "fastify": "^5.7.1",
    "handlebars": "^4.7.8",
    "ioredis": "^5.9.0",
    "jose": "^6.1.3",
    "mongoose": "^9.1.4",
    "nodemailer": "^7.0.12",
    "otp-generator": "^4.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.20",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.27",
    "@types/nodemailer": "^7.0.4",
    "@types/otp-generator": "^4.0.2",
    "@types/passport-google-oauth20": "^2.0.17",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-istanbul": "4.0.17",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "openssl": "^2.0.0",
    "prettier": "^3.7.4",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "pnpm": {
    "overrides": {
      "fastify": "5.7.1"
    }
  },
  "vitest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-vitest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.25.0+sha512.5e82639027af37cf832061bcc6d639c219634488e0f2baebe785028a793de7b525ffcd3f7ff574f5e9860654e098fe852ba8ac5dd5cefe1767d23a020a92f501"
}
