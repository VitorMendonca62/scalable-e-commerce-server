  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:prod": "node dist/main",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --config jest.config.json",
    "test:unit": "cross-env NODE_ENV=test jest --config jest.config.json --testPathPattern=\".unit.spec.ts\"",
    "test:unit:cov": "cross-env NODE_ENV=test jest --coverage --config jest.config.json --testPathPattern=\".unit.spec.ts\"",
    "jwt:keys:generate": "openssl genrsa -out auth-private.pem 2048 && openssl rsa -in auth-private.pem -pubout -out auth-public.pem && openssl genrsa -out reset-pass-private.pem 2048 && openssl rsa -in reset-pass-private.pem -pubout -out reset-pass-public.pem",
    "jwt:keys:clean": "rm auth-private.pem auth-public.pem reset-pass-private.pem reset-pass-public.pem"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
